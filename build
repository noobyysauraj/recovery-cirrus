
#!/bin/bash
cd /tmp/rec
rm -rf .repo
source build/envsetup.sh
lunch omni_merlin-eng #edit this
export SKIP_API_CHECKS=true ##use additional flags if you need(optional)
export SKIP_ABI_CHECKS=true ##use additional flags if you need(optional)
export ALLOW_MISSING_DEPENDENCIES=true ##use additional flags if you need(optional)
#necessary flags
export CCACHE_DIR=/tmp/ccache
export CCACHE_EXEC=$(which ccache)
export USE_CCACHE=1
ccache -M 40G
#ccache -o compression=true
ccache -z
ccache -c

mka recoveryimage

curl --upload-file ./out/otarget/product/merlin/recovery.img https://transfer.sh/  #edit this
curl --upload-file ./out/target/product/merlin/SHRP_AddonRescue*zip https://transfer.sh/  #edit this
curl --upload-file ./out/target/product/merlin/SHRP_v3.1_*zip https://transfer.sh/ #edit this
echo "

Uploaded All files successfully!"
#start rn

curl -F "file_upload=@out/otarget/product/merlin/recovery.img" https://api.uploadgram.me/upload
curl -F "file_upload=@out/otarget/product/merlin/SHRP_AddonRescue*zip" https://api.uploadgram.me/upload
curl -F "file_upload=@out/otarget/product/merlin/SHRP_v3.1_*zip" https://api.uploadgram.me/upload#
#
